<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="izo.wtamanagementsystem.repository.ProgramRepository">
    <select id="selectBoard">
        select * from program
    </select>

    <insert id="insert" parameterType="izo.wtamanagementsystem.model.Program">
        insert into program (id_program,program_name,prog_desc)
        values (#{idProgram},#{programName}, #{progDesc})

    </insert>
    <update id="update">
        update program
        set programName = #{programName},
            progDesc = #{progDesc}
        where idProgram = #{idProgram}
    </update>

    <delete id="delete">
        delete from program
        where idProgram = #{idProgram}
    </delete>
</mapper>
